@model Agenda_B.Models.Formulario

@{
    ViewData["Title"] = "Details";
}

<div class="container border bg-light">
    <h6>Consulta</h6>
    <div class="row">
        <div class="col-4">
            <p> <strong>@Html.DisplayNameFor(model => model.Titulo): </strong> @Html.DisplayFor(model => model.Titulo)</p>
        </div>
        <div class="col-4">
            <p> <strong>@Html.DisplayNameFor(model => model.Leido): </strong> @Html.DisplayFor(model => model.Leido)</p>
        </div>
        <div class="col-4">
            <p> <strong>@Html.DisplayNameFor(model => model.Fecha): </strong> @Html.DisplayFor(model => model.Fecha)</p>
        </div>
    </div>
    <div class="row">
        <div class="col-4">
            <p> <strong>@Html.DisplayNameFor(model => model.Nombre): </strong> @Html.DisplayFor(model => model.NombreCompleto)</p>
        </div>
        <div class="col-4">
            <p> <strong>@Html.DisplayNameFor(model => model.Email): </strong> @Html.DisplayFor(model => model.Email)</p>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <p> <strong>@Html.DisplayNameFor(model => model.Mensaje): </strong> @Html.DisplayFor(model => model.Mensaje)</p>
        </div>
    </div>
</div>
<div>
    <a asp-controller="Home" asp-action="Index">Regrasar al home</a>
</div>
